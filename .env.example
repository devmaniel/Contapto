VITE_SUPABASE_URL=KEY
VITE_SUPABASE_ANON_KEY=KEY
VITE_JWT_SECRET=KEY
# JWT expiration is controlled by Supabase server settings (currently 40 minutes)
# This value is for reference/documentation only - actual expiration comes from the JWT token
VITE_JWT_EXPIRATION_MINUTES=40

# Blockchain - Owner Private Key (for auto-confirm transactions, no MetaMask popup)
# Get this from your blockchain/.env file (same as PRIVATE_KEY there)
# ⚠️ WARNING: Never commit this to git! Keep it secret!
VITE_OWNER_PRIVATE_KEY=your_private_key_here
